{
  "name": "Subclass",
  "src_dir": "src",
  "build_dir": "build",
  "release_dir": "release",
  "doc_dir": "doc/api",
  "lib": {
    "dir": {
      "src": "<%= config.src_dir %>",
      "build": "<%= config.build_dir %>/lib",
      "release": "<%= config.release_dir %>/lib"
    },
    "content": {
      "main": "Subclass.js",
      "tools": {
        "dir": "Tools",
        "files": [
          "<%= config.lib.content.tools.dir %>/Tools.js",
          "<%= config.lib.content.tools.dir %>/ConverterTools.js",
          "<%= config.lib.content.tools.dir %>/**/*.js"
        ]
      },
      "class": {
        "dir": "Class",
        "files": [
          "<%= config.lib.content.class.dir %>/ClassManager.js",
          "<%= config.lib.content.class.dir %>/ClassManagerAPI.js",
          "<%= config.lib.content.class.dir %>/ClassType.js",
          "<%= config.lib.content.class.dir %>/ClassDefinition.js",
          "<%= config.lib.content.class.dir %>/*.js",
          "<%= config.lib.content.class.dir %>/Type/Class/Class.js",
          "<%= config.lib.content.class.dir %>/Type/Class/**/*.js",
          "<%= config.lib.content.class.dir %>/Type/AbstractClass/AbstractClass.js",
          "<%= config.lib.content.class.dir %>/Type/AbstractClass/**/*.js",
          "<%= config.lib.content.class.dir %>/Type/Interface/Interface.js",
          "<%= config.lib.content.class.dir %>/Type/Interface/**/*.js",
          "<%= config.lib.content.class.dir %>/Type/Trait/Trait.js",
          "<%= config.lib.content.class.dir %>/Type/Trait/**/*.js",
          "<%= config.lib.content.class.dir %>/Type/Config/Config.js",
          "<%= config.lib.content.class.dir %>/Type/Config/**/*.js",
          "<%= config.lib.content.class.dir %>/Type/**/*.js",
          "<%= config.lib.content.class.dir %>/Error/**/*.js",
          "<%= config.lib.content.class.dir %>/**/*.js"
        ]
      },
      "property": {
        "dir": "Property",
        "files": [
          "<%= config.lib.content.property.dir %>/PropertyManager.js",
          "<%= config.lib.content.property.dir %>/CustomTypesManager.js",
          "<%= config.lib.content.property.dir %>/PropertyType.js",
          "<%= config.lib.content.property.dir %>/PropertyDefinition.js",
          "<%= config.lib.content.property.dir %>/PropertyAPI.js",
          "<%= config.lib.content.property.dir %>/Type/Collection/CollectionType.js",
          "<%= config.lib.content.property.dir %>/Type/Collection/CollectionManager.js",
          "<%= config.lib.content.property.dir %>/Type/Collection/*.js",
          "<%= config.lib.content.property.dir %>/Type/Collection/*Collection/*Type.js",
          "<%= config.lib.content.property.dir %>/Type/Collection/*Collection/*.js",
          "<%= config.lib.content.property.dir %>/Type/**/*.js",
          "<%= config.lib.content.property.dir %>/**/*.js"
        ]
      },
      "error": {
        "dir": "Error",
        "files": [
          "<%= config.lib.content.error.dir %>/Error.js",
          "<%= config.lib.content.error.dir %>/**/*.js"
        ]
      },
      "service": {
        "dir": "Service",
        "files": [
          "<%= config.lib.content.service.dir %>/ServiceManager.js",
          "<%= config.lib.content.service.dir %>/**/*.js"
        ]
      },
      "parameter": {
        "dir": "Parameter",
        "files": [
          "<%= config.lib.content.parameter.dir %>/ParameterManager.js",
          "<%= config.lib.content.parameter.dir %>/**/*.js"
        ]
      },
      "module": {
        "dir": "Module",
        "files": [
          "<%= config.lib.content.module.dir %>/Module.js",
          "<%= config.lib.content.module.dir %>/**/*.js"
        ]
      },
      "event": {
        "dir": "Event",
        "files": [
          "<%= config.lib.content.event.dir %>/EventManager.js",
          "<%= config.lib.content.event.dir %>/**/*.js"
        ]
      }
    },
    "files": [
      "<%= config.lib.content.main %>",
      "<%= config.lib.content.tools.files %>",
      "<%= config.lib.content.error.files %>",
      "<%= config.lib.content.module.files %>",
      "<%= config.lib.content.event.files %>",
      "<%= config.lib.content.property.files %>",
      "<%= config.lib.content.class.files %>",
      "<%= config.lib.content.service.files %>",
      "<%= config.lib.content.parameter.files %>"
    ],
    "files_release": {
      "normal": "<%= config.lib.dir.release %>/<%= config.name %>.js",
      "minimized": "<%= config.lib.dir.release %>/<%= config.name %>.min.js"
    }
  },
  "demo": {
    "dir": {
      "src": "demo",
      "build": "<%= config.build_dir %>/demo",
      "release": "<%= config.release_dir %>/demo"
    },
    "index": "index.html",
    "files": "/**/*"
  }
}